#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/imadh/Downloads/finance-advisor (5)/backend/node_modules/.pnpm/yahoo-finance2@2.13.3/node_modules/yahoo-finance2/bin/node_modules:/mnt/c/Users/imadh/Downloads/finance-advisor (5)/backend/node_modules/.pnpm/yahoo-finance2@2.13.3/node_modules/yahoo-finance2/node_modules:/mnt/c/Users/imadh/Downloads/finance-advisor (5)/backend/node_modules/.pnpm/yahoo-finance2@2.13.3/node_modules:/mnt/c/Users/imadh/Downloads/finance-advisor (5)/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/imadh/Downloads/finance-advisor (5)/backend/node_modules/.pnpm/yahoo-finance2@2.13.3/node_modules/yahoo-finance2/bin/node_modules:/mnt/c/Users/imadh/Downloads/finance-advisor (5)/backend/node_modules/.pnpm/yahoo-finance2@2.13.3/node_modules/yahoo-finance2/node_modules:/mnt/c/Users/imadh/Downloads/finance-advisor (5)/backend/node_modules/.pnpm/yahoo-finance2@2.13.3/node_modules:/mnt/c/Users/imadh/Downloads/finance-advisor (5)/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  --experimental-json-modules --experimental-vm-modules "$basedir/../../bin/yahoo-finance.js" "$@"
else
  exec node  --experimental-json-modules --experimental-vm-modules "$basedir/../../bin/yahoo-finance.js" "$@"
fi
